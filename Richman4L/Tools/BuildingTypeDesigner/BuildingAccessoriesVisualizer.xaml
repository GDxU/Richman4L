<UserControl x:Class="BuildingTypeDesigner.BuildingAccessoriesVisualizer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BuildingTypeDesigner"
             mc:Ignorable="d" Height="198.851" Width="345.977">
	<Grid Background="#FFACECE3">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="10"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<TextBox x:Name="NameTextBox" TextWrapping="Wrap" Text="Name" FontSize="14" Background="{x:Null}"/>
		<StackPanel x:Name="PropertiesPanel" Grid.Row="2"/>

	</Grid>
</UserControl>
